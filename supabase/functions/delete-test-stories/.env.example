# Configuración de la Edge Function para limpieza de historias de prueba

# URL de tu proyecto Supabase
SUPABASE_URL=your-supabase-url.supabase.co

# Clave de servicio de Supabase (mantener en secreto)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Clave secreta para autenticar las llamadas a la función (cambiar en producción)
CLEANUP_API_KEY=your-secure-api-key-12345

# Nivel de registro (debug, info, warn, error)
LOG_LEVEL=debug
